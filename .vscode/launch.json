{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Next.js",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "dev"],
        "port": 9229,
        "skipFiles": ["<node_internals>/**"],
        "outputCapture": "std",
        "outFiles": [
            "${workspaceFolder}/**/*.(m|c|)js",
            "!**/node_modules/**"
        ]
      },
      {
        "type": "msedge",
        "request": "launch",
        "name": "Debug Frontend",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}"
      }
    ]
  }
  